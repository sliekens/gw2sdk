{
  "id": "github-cli",
  "version": "1.0.0",
  "dependsOn": {
    "ghcr.io/devcontainers/features/github-cli:1": {}
  },
  "mounts": [
    {
      "type": "volume",
      "source": "gh-config",
      "target": "/home/vscode/.config/gh"
    },
    {
      "type": "volume",
      "source": "gh-state",
      "target": "/home/vscode/.local/share/gh"
    }
  ]
}
