<Configuration>
  <Include>[GW2SDK]*</Include> <!-- [Assembly-Filter]Type-Filter -->
  <Exclude>[GW2SDK]JetBrains.Annotations.*</Exclude>
  <ExcludeByAttribute>Obsolete,GeneratedCodeAttribute,CompilerGeneratedAttribute</ExcludeByAttribute>
  <ExcludeByFile>**/*.generated.cs,**/*.g.cs</ExcludeByFile>
  <SingleHit>false</SingleHit>
  <DeterministicReport>true</DeterministicReport>

  <!-- UseSourceLink generates results with filename=https://raw.githubusercontent.com/etc instead of local file paths,
        which makes report generation super slow and network intensive. -->
  <UseSourceLink>false</UseSourceLink> 
</Configuration>
